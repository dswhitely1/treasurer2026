{
  "name": "treasurer-api",
  "version": "0.1.0",
  "description": "Treasurer API - Express TypeScript backend with Prisma, JWT auth, and OpenAPI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": ["api", "express", "typescript", "prisma"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "express": "^4.21.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.4",
    "prisma": "^5.22.0",
    "eslint": "^9.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@vitest/coverage-v8": "^2.1.4",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  }
}
